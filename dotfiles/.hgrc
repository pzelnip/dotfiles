[alias]
revertnobackup = revert --no-backup
pushcurrentbranch = !$HG push -b `$HG branch` --new-branch
commitcount = !hg log --template "{author|person}\n" | sort | uniq -c | sort -nr
shortlog = commitcount
sd = !$HG status ; $HG diff

